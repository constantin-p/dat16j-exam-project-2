                <Button fx:id="submitButton"
                    text="OK"
                    defaultButton="true"
                    onAction="#handleOKAction" />
            </children>
            <padding>
                <Insets
                    bottom="8.0"
                    top="8.0" />
            </padding>
        </HBox>

        <HBox
            alignment="CENTER_RIGHT"
            GridPane.columnIndex="3"
            GridPane.rowIndex="22">
            <children>
                <Button fx:id="printButton"
                    text="Print"
                    onAction="#handlePrintAction" />
            </children>
            <padding>
                <Insets
                    bottom="8.0"
                    top="8.0" />
            </padding>
        </HBox>
    </children>
</GridPane>
